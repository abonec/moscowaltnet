<article>
	<header>
		<h1><%= @event["title"]["$t"] %></h1>
		<div class="postmeta"><time datetime="<%= @event["gd$when"][0]["startTime"] %>"><%= Time.parse(@event["gd$when"][0]["startTime"]).ctime %></time></div>
		
	</header>
	<p>Место: <%= @event["gd$where"][0]["valueString"] %><a href="http://maps.google.com/maps?q=<%= @event["gd$where"][0]["valueString"] %>" target="new">(map)</a></p>
	<p><pre cols="85"><%= @event["content"]["$t"] %></pre></p>
</article>